{
  "name": "@octorelease/github",
  "version": "0.1.0",
  "description": "Octorelease plugin to perform actions related to GitHub.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -b && npm run lint",
    "lint": "eslint src",
    "pack": "esbuild src/index.ts --bundle --outfile=../../dist/github.js --platform=node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postversion": "cd ../.. && npm install --package-lock-only --ignore-scripts --no-audit",
    "watch": "tsc -w"
  },
  "keywords": [
    "github",
    "octorelease",
    "plugin",
    "publish"
  ],
  "author": "t1m0thyj",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@actions/github": "^5.1.0",
    "@actions/glob": "^0.4.0",
    "@octokit/plugin-enterprise-server": "^12.1.0",
    "delay": "^5.0.0",
    "mime-types": "^2.1.27",
    "semver": "^6.3.0"
  },
  "peerDependencies": {
    "@octokit/request-error": "^2.1.0",
    "@octorelease/core": "^0.1.0"
  }
}
